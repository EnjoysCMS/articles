{
    "name": "enjoyscms/articles",
    "description": "",
    "type": "enjoyscms-module",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EnjoysCMS\\Articles\\": "src/",
            "EnjoysCMS\\Module\\Articles\\Composer\\Scripts\\": "scripts/"
        }
    },
    "require": {
        "php": "^8.1",
        "enjoyscms/core": "^6.0@dev",
        "enjoyscms/admin": "^6.0@dev",
        "enjoyscms/elfinder": "^6.0@dev",
        "gedmo/doctrine-extensions": "^3.9",
        "beberlei/doctrineextensions": "^1.3"
    },
    "extra": {
        "module-name": "Статьи",
        "admin-links": {
            "@articles_list": "Обзор (Список)",
            "@articles_add": "Добавить статью",
            "@articles_category_list": "Категории"
        }
    },
    "scripts": {
        "articles-assets-install": "EnjoysCMS\\Module\\Articles\\Composer\\Scripts\\AssetsInstallCommand",
        "post-install-cmd": [
            "@print-module-name Articles",
            "@articles-assets-install"
        ],
        "post-update-cmd": [
            "@print-module-name Articles",
            "@articles-assets-install"
        ]
    }
}
